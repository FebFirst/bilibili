            <!--<column name="follower_id"/>-->
        <!--</many-to-one>-->
        <!--<many-to-one name="userByFolloweeId" class="com.pojo.User">-->
            <!--<column name="followee_id"/>-->
        <!--</many-to-one>-->
    </class>
</hibernate-mapping>